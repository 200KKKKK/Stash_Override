#!name=CustomRules
#!desc=自动添加自定义分流规则和策略组
#!author=yourname
#!icon=https://example.com/icon.png

http:
  mitm:
    - "api.stash.example.com"
  script:
    - match: ^https?://api\.stash\.example\.com/config
      name: AddCustomRules
      type: response
      require-body: true
      timeout: 10

script-providers:
  AddCustomRules:
    url: https://raw.githubusercontent.com/200KKKKK/Stash_Override/main/addCustomRules.js
    interval: 86400

